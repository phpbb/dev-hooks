{
    "autoload": {
        "psr-4": {
            "Phpbb\\DevHooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpbb\\DevHooks\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1.0",
        "ext-curl": "*",
        "chobie/jira-api-restclient": "2.0.*@dev",
        "knplabs/github-api": "^3.13",
        "pimple/pimple": "^3.5",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
